{
  "name": "ngx-pipes",
  "version": "3.0.0",
  "author": "Dan Revah",
  "description": "Useful angular pipes",
  "license": "MIT",
  "scripts": {
    "start": "ng serve",
    "test": "ng test",
    "test-travis": "ng test --karma-config karma.conf.js --watch=false",
    "build": "ng build",
    "build:lib": "ng-packagr",
    "lint": "ng lint --type-check",
    "release": "standard-version",
    "deploy": "npm run build:lib && npm publish --access=public dist",
