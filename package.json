    "prettier:write": "prettier --write './src/**/*.ts'",
    "prettier:check": "prettier -l './src/**/*.ts'"
  },
  "keywords": [
    "ngx",
    "ng",
    "angular",
    "pipes",
    "filters",
    "library",
    "ngx pipes",
    "angular filters",
    "angular pipes"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/danrevah/ngx-pipes.git"
  },
  "peerDependencies": {
    "@angular/core": "^14.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.0",
    "@angular/cli": "^14.0.0",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/compiler-cli": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@types/jasmine": "^2.8.2",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^12.2.0",
    "braces": "2.3.2",
    "codelyzer": "^6.0.2",
    "core-js": "^2.6.5",
    "husky": "^1.3.1",
    "jasmine-core": "~3.10.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lint-staged": "^7.3.0",
    "moment": "^2.24.0",
    "ng-packagr": "^14.0.0",
    "prettier": "^1.16.4",
    "protractor": "^7.0.0",
    "puppeteer": "^1.20.0",
    "rxjs": "6.6.7",
    "standard-version": "^8.0.1",
