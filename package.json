{
  "name": "ngx-pipes",
  "version": "2.4.3",
  "author": "Dan Revah",
  "description": "Useful angular2 pipes",
  "license": "MIT",
  "scripts": {
    "start": "ng serve",
    "test": "ng test",
    "test-travis": "ng test --karma-config karma.conf.js --watch=false",
    "build": "ng build",
    "build:lib": "ng-packagr",
    "lint": "ng lint --type-check",
    "release": "standard-version",
    "deploy": "npm run build:lib && npm publish --access=public dist",
    "precommit": "lint-staged",
    "prettier:write": "prettier --write './src/**/*.ts'",
    "prettier:check": "prettier -l './src/**/*.ts'"
  },
  "keywords": [
    "ngx",
    "ng",
    "angular",
    "pipes",
    "filters",
    "library",
    "ngx pipes",
    "angular filters",
    "angular pipes"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/danrevah/ngx-pipes.git"
  },
  "peerDependencies": {
    "@angular/core": "^4.0.1 || ^5.0.0 || ^6.0.0 || ^7.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.3",
    "@angular/cli": "^7.3.0",
    "@angular/common": "^7.2.6",
    "@angular/compiler": "^7.2.6",
    "@angular/compiler-cli": "^7.2.6",
    "@angular/core": "^7.2.6",
    "@angular/platform-browser": "^7.2.6",
    "@angular/platform-browser-dynamic": "^7.2.6",
    "@types/jasmine": "^2.8.2",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^10.0.0",
    "braces": "2.3.2",
    "codelyzer": "^4.5.0",
    "core-js": "^2.6.5",
    "husky": "^1.3.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.0",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "^7.3.0",
    "ng-packagr": "^4.7.1",
    "prettier": "^1.16.4",
    "protractor": "^5.4.2",
    "rxjs": "6.4.0",
    "standard-version": "^5.0.0",
    "tsickle": "^0.34.3",
    "tslint": "^5.12.1",
    "typescript": "^3.2.4",
    "zone.js": "^0.8.29"
  }
